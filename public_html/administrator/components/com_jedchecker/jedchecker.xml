	<name>COM_JEDCHECKER</name>
	<author>Compojoom.com</author>
	<creationDate>2013-11-05</creationDate>
	<copyright>(C) 2012 compojoom.com</copyright>
	<authorEmail>daniel@compojoom.com</authorEmail>
