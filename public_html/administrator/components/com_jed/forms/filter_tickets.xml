<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Joomla\Component\Jed\Administrator\Field">
    <fields name="filter">

        <field
                name="search"
                type="text"
                hint="JSEARCH_FILTER"
                label=""
        />

        <field
                name="published"
                type="list"
                label="JPUBLISHED"
                default=""
                size="1"
                onchange="this.form.submit();"
        >
            <option value="">JOPTION_SELECT_PUBLISHED</option>
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
        </field>

        <field
                name="extension"
                type="extensions"
                label="COM_JED_EXTENSION"
                default=""
                size="1"
                onchange="this.form.submit();"
        >
            <option value="">COM_JED_FILTER_EXTENSION</option>
        </field>

        <field
                name="developer"
                type="text"
                label="COM_JED_EXTENSIONS_DEVELOPER_LABEL"
                hint="COM_JED_EXTENSIONS_DEVELOPER_LABEL"
                autocomplete="off"/>

        <field
                name="developer_id"
                type="hidden"
                default=""
        />

    </fields>

    <fields name="list">

        <field name="limit"
               type="limitbox"
               default="25"
               onchange="this.form.submit();"
        />

    </fields>
</form>
