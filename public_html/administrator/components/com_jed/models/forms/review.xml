<?xml version="1.0" encoding="utf-8"?>
<form>
    <fieldset name="review_details">
        <field
                name="id"
                type="hidden"
                default="0"
                required="true"
                readonly="true"
        />

        <field
                name="published"
                type="list"
                label="JPUBLISHED"
                default=""
                size="1"
        >
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
        </field>

        <field
                name="actions"
                type="hidden"
                multiple="true"
        />

        <field
                name="lft"
                type="hidden"
                filter="unset"
        />

        <field
                name="rgt"
                type="hidden"
                filter="unset"
        />
    </fieldset>
</form>
